{
  "name": "json-api-builder",
  "version": "1.0.0",
  "description": "Declaratively build a JSON API-compliant API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "local-server": "nodemon --watch server index.js | bunyan -o short",
